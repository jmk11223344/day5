<button onclick="onpush()">
    <span id="b1">버튼이 눌리지 않음</span></button>

<script>
function onpush(){
    window.open("./page1.html","_self");
    return;
    let b = document.getElementById("b1");
    if(b.textContent =="버튼이 눌림"){
        b.textContent="버튼 안눌림";
    }else{
        b.textContent = "버튼이 눌림";
    }
    return;
}

    let e1 = document.getElementById("foo");
    e1.textContent = "멋진 사람"
</script>

